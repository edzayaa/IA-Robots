{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@layouts/*": ["./layouts/*"],
      "@components/*": ["./components/*"],
      "@styles/*": ["./styles/*"],
      "@assets/*": ["./assets/*"],
      "@/*": ["./*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
